<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSwipeRefreshLayout">
        <attr name="refreshMode" format="enum">
            <enum name="ContentAndRefreshDown" value="0"/>
            <enum name="RefreshOverContent" value="1"/>
            <enum name="ContentOverRefresh" value="2"/>
            <enum name="RefreshHalfDown" value="3"/>
        </attr>
        <attr name="freshLayout" format="enum">
            <enum name="content" value="0"/>
            <enum name="refresh" value="1"/>
        </attr>
    </declare-styleable>
</resources>